    <script>
        var a = parseInt(prompt("Enter a number."));
        var b = parseInt(prompt("Enter second number."));
        alert("Sum is  " + (a + b));
        document.write("Value of a = " + a);
        document.write("<br>");
        document.write("Value of b = " + b);
        document.write("<br>");
        document.write("Sum of a and b = " + (a+b));
        document.write("<br>");
    </script>
